<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="eda.eda" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:anyDensity="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".MainActivity"
            android:label="@string/navigation_friendcollect">
        </activity>

        <activity
            android:name=".MasterCollectActivity"
            android:label="@string/navigation_mastercollect">
        </activity>

        <activity
            android:name=".BrandCollectActivity"
            android:label="@string/navigation_brandcollect">
        </activity>
    </application>

</manifest>
